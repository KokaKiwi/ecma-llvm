#[tokens("keywords",
    VAR         =   'var',
)];

#[tokens("symbols",
    SEMICOLON   =   ';',
)];

#[tokens("values",
    IDENT       =   r'[a-zA-Z_][a-zA-Z0-9_]*',
)];

#[start]
module ::=
    statements,
;

statements ::=
    statements statement,
;

statement ::=
    var_statement ';',
;

var_statement ::=
    'var' IDENT,
;
